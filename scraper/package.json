    "fmt:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\""
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "@types/jsdom": "^27.0.0",
    "@eslint/js": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "prettier": "^3.7.1",
    "typescript-eslint": "^8.48.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "jsdom": "^27.2.0",
    "playwright": "^1.57.0",
    "zod": "^4.1.13"
  }
